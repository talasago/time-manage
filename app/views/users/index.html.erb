<% provide(:title, 'ユーザー一覧') %>
<h1>ユーザー一覧</h1>

<div class="wrapper">
  <div class="container">
    <%= will_paginate :previous_label => "&lt&lt", :next_label => '&gt&gt' %>

    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= link_to user.name, user %>
        </li>
      <% end %>
    </ul>

    <%= will_paginate :previous_label => "&lt&lt", :next_label => "&gt&gt"%>
  </div>
</div>
